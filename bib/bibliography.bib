	Journal = {International Journal of Energy Research},
	Keywords = {Stirling engine optimization, Direct Method, first law for processes with finite speed, PV/Px Stirling diagram, regenerator efficiency, pressure losses evaluation, scheme of computation, experimental validation},
	Number = {7},
	Pages = {589--609},
	Publisher = {John Wiley & Sons, Ltd.},
	Title = {Application of the Direct Method to irreversible Stirling cycles with finite speed},
	Url = {http://dx.doi.org/10.1002/er.806},
