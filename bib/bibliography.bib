	Issn = {0306-2619},
	Journal = {Applied Energy},
	Keywords = {Solar field layout},
	Number = {11},
	Pages = {4055 - 4066},
	Title = {Energy management in solar thermal power plants with double thermal storage system and subdivided solar field},
	Url = {http://www.sciencedirect.com/science/article/pii/S0306261911002583},
	Volume = {88},
	Year = {2011},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0306261911002583},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.apenergy.2011.04.036}}

@article{Bilgen2005,
	Abstract = {A solar chimney system for power production at high latitudes has been designed and its performance has been evaluated. A mathematical model and a code on \{MATLAB\} platform have been developed based on monthly average meteorological data and thermodynamic cycle. The thermal performance of a 5 \{MW\} nominal power production plant at three locations in Canada, namely Ottawa, Winnipeg and Edmonton, is studied. The sloped collector field is built at suitable mountain hills, which also functions as a chimney. Then a short vertical chimney is added to install the vertical axis air turbine. The results showed that solar chimney power plants at high latitudes may have satisfactory thermal performance and produce as much as 85% of the same plants in southern locations with horizontal collector field. The overall thermal performance of these plants is a little less than 0.5%. },
