	Issn = {1290-0729},
	Journal = {International Journal of Thermal Sciences},
	Keywords = {Artificial neural network},
	Pages = {19 - 30},
	Title = {Investigation of convective and radiative heat losses from modified cavity based solar dish steam generator using \{ANN\}},
	Url = {http://www.sciencedirect.com/science/article/pii/S1290072914002300},
	Volume = {87},
	Year = {2015},
