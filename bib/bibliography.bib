	Keywords = {Thermal oil},
	Pages = {1298 - 1309},
	Title = {Performance study of a solar-assisted organic Rankine cycle using a dish-mounted rectangular-cavity tubular solar receiver},
	Url = {http://www.sciencedirect.com/science/article/pii/S1359431116313606},
	Volume = {108},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1359431116313606},
