	Issn = {0038-092X},
	Journal = {Solar Energy},
	Keywords = {Numerical modeling},
	Note = {\{ISES\} Solar World Congress 2013 (SWC2013) Special Issue},
	Pages = {78 - 93},
	Title = {Combined heat loss analysis of solar parabolic dish -- modified cavity receiver for superheated steam generation},
	Url = {http://www.sciencedirect.com/science/article/pii/S0038092X15002169},
	Volume = {121},
	Year = {2015},
