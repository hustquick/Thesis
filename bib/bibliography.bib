	Pages = {308 - 319},
	Title = {Experimental investigation of porous disc enhanced receiver for solar parabolic trough collector},
	Url = {http://www.sciencedirect.com/science/article/pii/S0960148114008428},
	Volume = {77},
	Year = {2015},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0960148114008428},
